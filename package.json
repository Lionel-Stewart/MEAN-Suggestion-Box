{
  "name": "suggestion-box",
  "version": "1.0.0",
  "description": "A MEAN Stack CRUD suggestion box app with authentication by Passport.js",
  "main": "server.js",
  "scripts": {
    "start": "node bin/www",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "server": "nodemon bin/www",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Lionel Stewart",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^5.4.16",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "rxjs-compat": "^6.4.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
